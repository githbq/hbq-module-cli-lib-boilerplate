{
    "name": "module-cli-lib",
    "version": "1.0.0",
    "description": "module-cli-lib",
    "main": "build/index.js",
    "bin": {
        "module-cli-lib": "./build/bin"
    },
    "scripts": {
        "start:build": "npm run tsc && node build",
        "start": "node run",
        "test": "jest --forceExit --detectOpenHandles --colors",
        "clear": "rimraf build",
        "tsc": "npm run clear && tsc",
        "tsc:w": "npm run tsc -- -w",
        "p": "npm run tsc && npm publish",
        "lint": "eslint --ext .ts src --fix"
    },
    "dependencies": {
        "fs-extra": "^9.1.0",
        "globby": "^11.0.2",
        "tslib": "^2.0.1",
        "vuepress": "^1.8.2",
        "yargs": "^15.4.1"
    },
    "devDependencies": {
        "@types/jest": "^26.0.13",
        "@types/node": "^12.12.53",
        "@types/yargs": "^15.0.5",
        "@typescript-eslint/eslint-plugin": "^4.15.2",
        "@typescript-eslint/parser": "^4.15.2",
        "eslint": "^7.21.0",
        "jest": "^26.4.2",
        "rimraf": "^2.7.1",
        "ts-jest": "^26.3.0",
        "ts-node": "^8.10.2",
        "typescript": "^3.9.7"
    },
    "keywords": [
        "module-cli-lib"
    ],
    "author": "hubiqing",
    "repository": "https://github.com/githbq/hbq-module-cli-lib-boilerplate.git",
    "engines": {
        "node": ">=8"
    },
    "license": "MIT"
